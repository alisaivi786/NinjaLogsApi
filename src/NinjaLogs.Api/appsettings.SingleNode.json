{
  "Storage": {
    "Provider": "SQLite",
    "ConnectionString": "",
    "LogsDirectory": "logs",
    "Deployment": {
      "Profile": "SingleNode",
      "NodeMode": "SingleWriter"
    },
    "SegmentedFile": {
      "DataDirectory": "data",
      "SegmentMaxBytes": 268435456,
      "ManifestFileName": "manifest.json"
    },
    "Retention": {
      "Enabled": true,
      "RetainDays": 30,
      "CleanupIntervalMinutes": 60
    },
    "IngestionPipeline": {
      "QueueCapacity": 20000,
      "BatchSize": 200,
      "WriterWorkers": 1,
      "MaxWriteRetries": 3,
      "RetryDelayMs": 200,
      "DeadLetterDirectory": "deadletter",
      "MaxPayloadBytes": 1048576,
      "MaxRequestsPerMinutePerApiKey": 600
    },
    "Connections": {
      "SQLite": "Data Source=logs/ninjalogs.db",
      "SqlServer": "",
      "PostgreSQL": ""
    }
  },
  "Licensing": {
    "Tier": "Community",
    "IsLicensed": false
  },
  "ApiKeys": {
    "IngestionKeys": ["dev-ingestion-key"]
  }
}
